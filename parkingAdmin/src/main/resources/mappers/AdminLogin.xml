<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="adminLogin">
	<select id="getAdminPwd" resultType="String" parameterType="com.parking.admin.vo.AdminVO">

		select
		adminPwd
		from admin
		where adminId=#{adminId} 


	</select>

	<select id="getAdminData" parameterType="com.parking.admin.vo.AdminVO"
		resultType="com.parking.admin.vo.AdminVO">

		select adminId as adminId,
		adminNum as adminNum
	
		from admin
		where
		adminId=#{adminId}
	</select>


</mapper>
